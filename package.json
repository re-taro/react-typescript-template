{
  "name": "react-typescript-template",
  "version": "1.0.0",
  "description": "Template for personal React projects",
  "keywords": [
    "react",
    "typescript",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/react-typescript-template"
  },
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://www.re-taro.dev"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "start": "vite preview",
    "lint": "eslint ./**/**/**/**/*.{js,jsx,ts,tsx,json,yaml,yml,toml}",
    "lint:fix": "npm run lint --fix",
    "fmt": "prettier -w -u .",
    "type": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.23",
    "twin.macro": "^2.8.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@emotion/babel-plugin-jsx-pragmatic": "0.1.5",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@vitejs/plugin-react": "1.2.0",
    "autoprefixer": "10.4.4",
    "babel-plugin-macros": "3.1.0",
    "eslint": "8.11.0",
    "eslint-config-re-taro": "1.0.8",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "postcss": "8.4.8",
    "prettier": "2.6.0",
    "rollup": "2.70.1",
    "typescript": "4.6.2",
    "vite": "2.8.6"
  }
}
